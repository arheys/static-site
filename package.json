{
  "name": "vue-static",
  "description": "A static site generator in Vue.js",
  "version": "0.0.0",
  "homepage": "",
  "main": "server",
  "keywords": [
    "Vue.js",
    "static-site-generator"
  ],
  "author": {
    "name": "Jon Paul Miles",
    "email": "codingfriend1@gmail.com"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "server"
  },
  "engines": {
    "node": ">=7.6.0",
    "yarn": ">= 0.18.0"
  },
  "scripts": {
    "serve": "http-server -p3000 dist",
    "generate": "nodemon --watch markdown --watch theme --watch templates -e md,js,vue,html,css,styl,scss,sass,xml the-magic/ssr.js",
    "watch": "gulp && webpack -w",
    "webpack": "webpack",
    "gulp": "gulp",
    "start": "npm-run-all --parallel watch generate serve",
    "build": "gulp && NODE_ENV=production webpack -p && node the-magic/ssr.js",
    "webp": "node webp.js"
  },
  "dependencies": {
    "del": "^3.0.0",
    "file-loader": "^1.1.11",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "gray-matter": "^4.0.1",
    "http-server": "^0.11.1",
    "klaw": "^2.1.1",
    "lodash.camelcase": "^4.3.0",
    "node-fetch": "^2.2.0",
    "showdown": "^1.8.6",
    "vue": "^2.5.2",
    "vue-analytics": "^5.12.2",
    "vue-meta": "https://github.com/codingfriend1/vue-meta.git#dev",
    "vue-router": "^2.8.1",
    "vue-server-renderer": "^2.5.2",
    "vue-stash": "^2.0.1-beta"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "compression-webpack-plugin": "^1.0.1",
    "copy-webpack-plugin": "^4.2.1",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^3.0.1",
    "gulp": "^3.9.1",
    "gulp-inject": "^4.2.0",
    "html-webpack-plugin": "^2.30.1",
    "imagemin": "^6.0.0",
    "imagemin-webp": "^4.1.0",
    "json-loader": "^0.5.4",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "progress-bar-webpack-plugin": "^1.10.0",
    "pug": "^2.0.0-rc.4",
    "raw-loader": "^0.5.1",
    "run-sequence": "^1.2.2",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "uglify-js": "^2.7.5",
    "uglifyjs-webpack-plugin": "^1.0.0-beta.1",
    "url-loader": "^0.5.7",
    "vue-loader": "10.3.0",
    "vue-template-compiler": "^2.5.2",
    "webpack": "^3.8.1",
    "webpack-async-await": "1.0.4",
    "webpack-merge": "^4.1.0",
    "webpack-node-externals": "^1.6.0"
  }
}
