{
  "name": "vue-static",
  "description": "A static site generator in Vue.js",
  "version": "0.0.0",
  "homepage": "",
  "main": "server",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "Jon Paul Miles",
    "email": "codingfriend1@gmail.com"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "server"
  },
  "engines": {
    "node": ">=7.6.0",
    "yarn": ">= 0.18.0"
  },
  "scripts": {
    "serve": "http-server -p3000 dist",
    "generate": "nodemon --watch markdown --watch themes --watch templates -e md,js,vue,html,css,styl,scss,sass,xml the-magic/ssr.js",
    "watch": "gulp && webpack -w",
    "webpack": "webpack",
    "gulp": "gulp",
    "start": "npm-run-all --parallel watch generate serve",
    "build": "gulp && NODE_ENV=production webpack -p && node the-magic/ssr.js"
  },
